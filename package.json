{
  "name": "Meisunry",
  "version": "1.0.8",
  "description": "A masonry image viewer",
  "main": "./mains/main.js",
  "scripts": {
    "start": "electron ./mains/main.js"
  },
  "author": "Jonah Grimm",
  "license": "MIT",
  "devDependencies": {
    "electron": "^26.1.0",
    "electron-builder": "^24.6.3"
  },
  "dependencies": {
    "electron-context-menu": "^3.6.1",
    "masonry-layout": "^4.2.2",
    "panzoom": "^9.4.3",
    "trash": "^8.1.1"
  },
  "iohook": {
    "targets": [
      "node-72",
      "electron-85"
    ],
    "platforms": [
      "win32",
      "darwin",
      "linux"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  },
  "build": {
    "win": {
      "icon": "/app-icons/logo.ico"
    },
    "asar": false
  }
}
